{
  "threshold": 0,
  "mode": "strict",
  "format": "dart",
  "reporters": [
    "html",
    "console"
  ],
  "ignore": [
    ".reports/**",
    ".vscode/**",
    ".fvm/**",
    ".cspell.json",
    ".prettierrc",
    ".prettierignore",
    ".markdownlint.json",
    ".markdownlintignore",
    ".markdown-link-check.json",
    ".jscpd.json",
    "analysis_options.yaml",
    "packages/*/.dart_tool/**",
    "packages/*/build/**",
    "packages/*/coverage/**",
    "packages/*/android/**",
    "packages/*/web/**",
    "packages/*/windows/**",
    "packages/*/.packages",
    "packages/*/.flutter-plugins",
    "packages/*/.flutter-plugins-dependencies",
    "packages/*/lib/generated_plugin_registrant.dart",
    ".words",
    ".coverage",
    "**/*.lock",
    "**/*.freezed.dart",
    "**/*.g.dart",
    "**/*.ttf",
    "**/*.bat",
    "packages/*/assets/i18n/**",
    "packages/*/test/snapshots/**",
    "packages/**/test/**"
  ],
  "absolute": true,
  "output": ".reports/jscpd"
}
